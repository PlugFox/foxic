# Firebase Configuration
# Copy this file to .env.local and fill in your actual values

# Firebase API Key
VITE_FIREBASE_API_KEY=your_firebase_api_key_here

# Firebase Auth Domain (usually your-project-id.firebaseapp.com)
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com

# Firebase Project ID
VITE_FIREBASE_PROJECT_ID=your-project-id

# Firebase Storage Bucket (usually your-project-id.appspot.com)
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com

# Firebase Messaging Sender ID
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id

# Firebase App ID
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Firebase Analytics Measurement ID (optional)
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Optional: App Version (automatically set during tag deployments)
VITE_APP_VERSION=0.0.1